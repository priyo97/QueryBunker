<p id="notice"><%= notice %></p>

<h1>Listing Qas</h1>

<table>
  <thead>
    <tr>
      <th>Questions</th>
      <th>Answer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qas.each do |qa| %>
      <tr>
        <td><%= qa.questions %></td>
        <td><%= qa.answer %></td>
        <td><%= link_to 'Show', qa %></td>
        <td><%= link_to 'Edit', edit_qa_path(qa) %></td>
        <td><%= link_to 'Destroy', qa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Qa', new_qa_path %>
